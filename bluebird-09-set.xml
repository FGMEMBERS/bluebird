<?xml version="1.0"?>

<PropertyList include="bluebird-base.xml">

 <sim>
	<description>Bluebird Explorer for v.0.9.10</description>
	<model>
		<path>Aircraft/bluebird/Models/bluebird-plib.xml</path>
	</model>

	<menubar>
		<default>
			<menu n="10">
				<label>Bluebird</label>
				<enabled type="bool">true</enabled>
				<item>
					<label>Systems</label>
					<binding>
						<command>nasal</command>
						<script>bluebird.showDialog1()</script>
					</binding>
				</item>
				<item>
					<label>Configuration</label>
					<binding>
						<command>nasal</command>
						<script>bluebird.showDialog2()</script>
					</binding>
				</item>
			</menu>
		</default>
	</menubar>

	<submodels>
		<serviceable>true</serviceable>
		<path>Aircraft/bluebird/Models/submodels-09.xml</path>
	</submodels>
 </sim>

 <nasal>
	<bluebird>
		<file>Aircraft/bluebird/Nasal/bluebird-base.nas</file>
		<file>Aircraft/bluebird/Nasal/bluebird-09.nas</file>
	</bluebird>
	<digitalPanel>
		<file>Aircraft/bluebird/Instruments-3d/digital2/digital2.nas</file>
	</digitalPanel>
 </nasal>
</PropertyList>
