		<condition>
			<less-than>
				<property>sim/model/bluebird/doors/door[5]/position-norm</property>
				<value>0.0284</value>
			</less-than>
		</condition>
		<volume>
			<factor>2</factor>
		</volume>
	</door>

<!-- gears -->
	<gear>
		<name>gear</name>
		<mode>in-transit</mode>
		<path>Sounds/gear.wav</path>
		<condition>
			<or>
				<and>
					<greater-than>
						<property>gear/gear[0]/position-norm</property>
						<value>0.01</value>
					</greater-than>
					<less-than>
						<property>gear/gear[0]/position-norm</property>
						<value>0.409</value>
					</less-than>
				</and>
				<and>
					<greater-than>
						<property>gear/gear[0]/position-norm</property>
						<value>0.411</value>
					</greater-than>
					<less-than>
						<property>gear/gear[0]/position-norm</property>
						<value>0.99</value>
					</less-than>
				</and>
			</or>
		</condition>
		<pitch>
			<internal>dt_play</internal>
			<offset>1.0</offset>
			<factor>-0.001</factor>
		</pitch>
		<volume>
			<factor>0.15</factor>
		</volume>
	</gear>

	<gear>
		<name>gear-up-lock</name>
		<path>Sounds/gear-lck.wav</path>
		<condition>
			<equals>
				<property>gear/gear[0]/position-norm</property>
				<value>0</value>
			</equals>
		</condition>
	</gear>

	<gear>
		<name>wheels-gear-plates</name>
		<mode>in-transit</mode>
		<path>Sounds/gear.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>gear/gear[1]/position-norm</property>
					<value>0.01</value>
				</greater-than>
				<less-than>
					<property>gear/gear[1]/position-norm</property>
					<value>0.42</value>
				</less-than>
			</and>
		</condition>
		<pitch>
			<factor>0.03</factor>
		</pitch>
		<volume>
			<factor>0.1</factor>
		</volume>
	</gear>

	<gear>
		<name>wheels-piston</name>
		<path>Aircraft/bluebird/Sound/pneumatic-door.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>gear/gear[1]/position-norm</property>
					<value>0.42</value>
				</greater-than>
				<less-than>
					<property>gear/gear[1]/position-norm</property>
					<value>0.99</value>
				</less-than>
			</and>
		</condition>
		<volume>
			<factor>0.1</factor>
		</volume>
	</gear>

	<gear>
		<name>wheels-up-lock</name>
		<path>Sounds/gear-lck.wav</path>
		<condition>
			<equals>
				<property>gear/gear[1]/position-norm</property>
				<value>0</value>
			</equals>
		</condition>
		<volume>
			<factor>0.4</factor>
		</volume>
	</gear>

<!-- ground contacts -->
	<landing>
		<name>vertical ground contact</name>
		<mode>once</mode>
		<path>Sounds/helicopter-skid.wav</path>
		<condition>
			<property>sim/model/bluebird/position/landing-wow</property>
		</condition>
	</landing>

	<contact-slow>
		<name>slow moving ground contact</name>
		<mode>looped</mode>
		<path>Sounds/squeal.wav</path>
		<condition>
			<property>sim/model/bluebird/position/squeal-wow</property>
		</condition>
		<volume>
			<property>velocities/abs-airspeed-kt</property>
			<factor>0.0005</factor>
			<max>0.1</max>
		</volume>
	</contact-slow>

	<contact-continuous>
		<name>continuous ground contact</name>
		<mode>looped</mode>
		<path>Sounds/rumble.wav</path>
		<condition>
			<and>
				<greater-than>
					<property>sim/model/bluebird/position/skid-wow</property>
					<value>0.05</value>
				</greater-than>
				<greater-than>
					<property>velocities/abs-airspeed-kt</property>
					<value>20</value>
				</greater-than>
			</and>
		</condition>
		<volume>
			<property>sim/model/bluebird/position/skid-wow</property>
		</volume>
		<pitch>
			<factor>0.01</factor>
		</pitch>
	</contact-continuous>

	<crash-structure-holding>
		<name>intense ground contact</name>
		<mode>once</mode>
		<path>Sounds/squeal.wav</path>
		<condition>
			<greater-than>
				<property>sim/model/bluebird/position/skid-wow</property>
				<value>1.0</value>
			</greater-than>
		</condition>
		<volume>
			<factor>2.5</factor>
		</volume>
		<pitch>
			<factor>0.1</factor>
		</pitch>
	</crash-structure-holding>

	<crash>
		<name>hard crash with damage</name>
		<mode>once</mode>
		<path>Sounds/crash.wav</path>
		<condition>
			<property>sim/model/bluebird/position/crash-wow</property>
		</condition>
	</crash>

 </fx>
</PropertyList>
