<?xml version="1.0"?>
<!--== Bluebird Explorer Hover Flyer  version 3.3 ==-->

<PropertyList>

 <path>velocity2.ac</path>

 <animation>
   <name>velocity-lit</name>
   <object-name>velocity.label</object-name>
   <object-name>velocity.dec</object-name>
   <object-name>vel.minus</object-name>
   <object-name>vel.101</object-name>
   <object-name>vel.102</object-name>
   <object-name>vel.103</object-name>
   <object-name>vel.104</object-name>
   <object-name>vel.105</object-name>
   <object-name>vel.106</object-name>
   <object-name>vel.kmph</object-name>
   <object-name>vel.kts</object-name>
   <object-name>vel.mph</object-name>
   <object-name>vel.mps</object-name>
   <object-name>throttle.label</object-name>
   <object-name>throttle.101</object-name>
   <object-name>throttle.102</object-name>
   <object-name>throttle.103</object-name>
   <object-name>throttle.percent</object-name>
 </animation>

 <animation>
   <type>select</type>
   <object-name>vel.minus</object-name>
   <condition>
     <less-than>
       <property>velocities/airspeed-kt</property>
       <value>0</value>
       </less-than>
   </condition>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>vel.101</object-name>
   <property>instrumentation/digital/velocity-whole</property>
   <factor>0.000001</factor>
   <step>100000</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>vel.102</object-name>
   <property>instrumentation/digital/velocity-whole</property>
   <factor>0.00001</factor>
   <step>10000</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>vel.103</object-name>
   <property>instrumentation/digital/velocity-whole</property>
   <factor>0.0001</factor>
   <step>1000</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>vel.104</object-name>
   <property>instrumentation/digital/velocity-whole</property>
   <factor>0.001</factor>
   <step>100</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>vel.105</object-name>
   <property>instrumentation/digital/velocity-whole</property>
   <factor>0.01</factor>
   <step>10</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>vel.106</object-name>
   <property>instrumentation/digital/velocity-whole</property>
   <factor>0.1</factor>
   <step>1</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>select</type>
   <object-name>vel.mps</object-name>
   <condition>
     <equals>
       <property>instrumentation/digital/velocity-mode</property>
       <value>0</value>
     </equals>
   </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>vel.kts</object-name>
   <condition>
     <equals>
       <property>instrumentation/digital/velocity-mode</property>
       <value>1</value>
     </equals>
   </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>vel.mph</object-name>
   <condition>
     <equals>
       <property>instrumentation/digital/velocity-mode</property>
       <value>2</value>
     </equals>
   </condition>
 </animation>

 <animation>
   <type>select</type>
   <object-name>vel.kmph</object-name>
   <condition>
     <equals>
       <property>instrumentation/digital/velocity-mode</property>
       <value>3</value>
     </equals>
   </condition>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>throttle.101</object-name>
   <property>instrumentation/digital/throttle-whole</property>
   <factor>0.001</factor>
   <step>100</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>throttle.102</object-name>
   <property>instrumentation/digital/throttle-whole</property>
   <factor>0.01</factor>
   <step>10</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>

 <animation>
   <type>textranslate</type>
   <object-name>throttle.103</object-name>
   <property>instrumentation/digital/throttle-whole</property>
   <factor>0.1</factor>
   <step>1</step>
   <axis>
     <x>1</x>
     <y>0</y>
     <z>0</z>
   </axis>
 </animation>


</PropertyList>