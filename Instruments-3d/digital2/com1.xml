 <animation>
	<type>material</type>
	<object-name>com1-buttons-lit-off</object-name>
	<emission>
		<factor-prop>sim/model/bluebird/systems/power-switch</factor-prop>
		<red>0.5</red>
		<green>0.25</green>
		<blue>0.125</blue>
	</emission>
 </animation>

 <animation>
	<type>material</type>
	<object-name>com1-buttons-lit-on</object-name>
	<emission>
		<factor-prop>sim/model/bluebird/lighting/buttons/com1-backlit</factor-prop>
		<red>0.5</red>
		<green>0.25</green>
		<blue>0.125</blue>
	</emission>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com1-lit</object-name>
	<condition>
		<and>
			<property>sim/model/bluebird/systems/power-switch</property>
			<property>instrumentation/comm[0]/power-btn</property>
		</and>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com1s-digits-lit</object-name>
	<condition>
		<and>
			<property>sim/model/bluebird/systems/power-switch</property>
			<property>instrumentation/comm[0]/power-btn</property>
		</and>
	</condition>
 </animation>

 <animation>
	<type>material</type>
	<object-name>com1s-digits-lit</object-name>
	<emission>
		<red>0.83</red>
		<green>0.30</green>
		<blue>0.04</blue>
	</emission>
	<ambient>
		<red>0.83</red>
		<green>0.36</green>
		<blue>0</blue>
	</ambient>
	<diffuse>
		<red>0.83</red>
		<green>0.36</green>
		<blue>0</blue>
	</diffuse>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com1.button.power.off</object-name>
	<condition>
		<not>
			<and>
				<property>sim/model/bluebird/systems/power-switch</property>
				<property>instrumentation/comm[0]/power-btn</property>
			</and>
		</not>
	</condition>
 </animation>

