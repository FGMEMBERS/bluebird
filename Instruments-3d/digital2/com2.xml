 <animation>
	<type>material</type>
	<object-name>com2-buttons-lit</object-name>
	<emission>
		<factor-prop>sim/model/bluebird/systems/power-switch</factor-prop>
		<red>0.5</red>
		<green>0.25</green>
		<blue>0.125</blue>
	</emission>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com2-lit</object-name>
	<condition>
		<and>
			<property>sim/model/bluebird/systems/power-switch</property>
			<property>instrumentation/comm[1]/power-btn</property>
		</and>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com2s-digits-lit</object-name>
	<condition>
		<and>
			<property>sim/model/bluebird/systems/power-switch</property>
			<property>instrumentation/comm[1]/power-btn</property>
		</and>
	</condition>
 </animation>

 <animation>
	<type>material</type>
	<object-name>com2s-digits-lit</object-name>
	<emission>
		<red>0.83</red>
		<green>0.30</green>
		<blue>0.04</blue>
	</emission>
	<ambient>
		<red>0.83</red>
		<green>0.36</green>
		<blue>0</blue>
	</ambient>
	<diffuse>
		<red>0.83</red>
		<green>0.36</green>
		<blue>0</blue>
	</diffuse>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com2.button.power.off</object-name>
	<condition>
		<or>
			<not>
				<property>sim/model/bluebird/systems/power-switch</property>
			</not>
			<not>
				<property>instrumentation/comm[1]/power-btn</property>
			</not>
		</or>
	</condition>
 </animation>

 <animation>
	<type>select</type>
	<object-name>com2.button.power.on</object-name>
	<condition>
		<property>instrumentation/comm[1]/power-btn</property>
	</condition>
 </animation>

