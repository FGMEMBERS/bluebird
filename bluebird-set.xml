 <input>
   <keyboard>
     <key n="9">
       <name>Ctrl-I</name>
       <desc>Show configuration dialog</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.showDialog()</script>
       </binding>
     </key>
     <key n="126">
       <name>Tilde~</name>
       <desc>Startup/Shutdown Master Power</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.toggle_power(1)</script>
       </binding>
     </key>
     <key n="32">
       <name>space</name>
       <desc>Engage/Disengage Primary engines</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.toggle_wave1()</script>
       </binding>
     </key>
     <key n="92">
       <name>\</name>
       <desc>Engage/Disengage CounterGrav and Fusion reactor</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.toggle_fusion()</script>
       </binding>
     </key>
     <key n="311">
       <name>KP7</name>
       <desc>Hover Up</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.hover_grav(1, 0.15, 1)</script>
       </binding>
     </key>
     <key n="305">
       <name>KP1</name>
       <desc>Hover Down</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.hover_grav(-1, 0.15, 1)</script>
       </binding>
     </key>
     <key n="362">
       <name>KP7</name>
       <desc>Hover Up</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.hover_grav(1, 0.15, 1)</script>
       </binding>
     </key>
     <key n="363">
       <name>KP1</name>
       <desc>Hover Down</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.hover_grav(-1, 0.15, 1)</script>
       </binding>
     </key>
     <key n="4">
       <name>Ctrl-D</name>
       <desc>Open/Close selected door</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.toggle_door()</script>
       </binding>
     </key>
     <key n="100">
       <name>d</name>
       <desc>Select next door</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.next_door()</script>
       </binding>
     </key>
     <key n="68">
       <name>D</name>
       <desc>Select previous door</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.previous_door()</script>
       </binding>
     </key>
     <key n="7">
       <name>Ctrl-G</name>
       <desc>Toggle gear deployment mode</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.toggle_gear_mode(-1)</script>
       </binding>
     </key>
     <key n="23">
       <name>Ctrl-W</name>
       <desc>Toggle wheel deployment mode</desc>
       <binding>
         <command>nasal</command>
         <script>bluebird.toggle_wheel_mode()</script>
       </binding>
