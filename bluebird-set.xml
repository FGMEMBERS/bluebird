   <view n="0">
     <internal>true</internal>
     <config>
       <!-- x/y/z == right/up/back -->
       <x-offset-m>0.0</x-offset-m>  	# starboard 
       <y-offset-m>1.47</y-offset-m> 	# elevation from ground
       <z-offset-m>-7.35</z-offset-m>	# forward
       <pitch-offset-deg>0.0</pitch-offset-deg>
     </config>
   </view>

