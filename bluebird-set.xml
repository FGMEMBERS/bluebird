     </key>
     <key>
       <name>E</name>
       <desc>Return and Sit down in cockpit</desc>
     </key>
     <key>
       <name>w/W</name>
       <desc>Walk Aft/Fore</desc>
     </key>
     <key>
       <name>L</name>
       <desc>Load next Livery</desc>
     </key>
     <key>
       <name>l</name>
       <desc>Livery Selection Dialog</desc>
     </key>
     <key>
       <name>Ctrl-Y</name>
       <desc>Exterior Material Dialogs</desc>
     </key>
     <key>
       <name>Ctrl-U</name>
       <desc>Interior Material Dialogs</desc>
     </key>

     <key>
       <name>Joystick</name>
       <desc>buttons:</desc>
     </key>
     <key>
       <name>flaps button</name>
       <desc>Inc/Dec Max Speed power level</desc>
     </key>
     <key>
       <name>elevator trim</name>
       <desc>Counter-grav VTOL up/down</desc>
     </key>
     <key>
       <name>fire trigger</name>
       <desc>Reverse direction - fly backwards</desc>
     </key>
