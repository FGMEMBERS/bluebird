<?xml version="1.0"?>
<!-- Bluebird Explorer Hovercraft  version 13.2 for FlightGear OSG only -->

<PropertyList>

<!-- instruments placement for bottom center console of bluebird cockpit
	3rd row, top    line: x=-8.106 z=1.133 y=+-0.246
	3rd row, bottom line: x=-8.060 z=1.048 y=+-0.249 
	row 3 rise/run= 0.085/0.046 = 1.85 = -28.4 deg 
-->

 <model>
	<name>COM1A</name>
	<path>Aircraft/bluebird/Instruments-3d/digital2/com1a.xml</path>
	<offsets>
		<x-m>-8.096</x-m>
		<y-m>0.027</y-m>
		<z-m>1.113</z-m>
		<pitch-deg>-28.65</pitch-deg>
		<w>512</w>
		<h>512</h>
	</offsets>
 </model>

 <model>
	<name>COM1S</name>
	<path>Aircraft/bluebird/Instruments-3d/digital2/com1s.xml</path>
	<offsets>
		<x-m>-8.096</x-m>
		<y-m>0.118</y-m>
		<z-m>1.113</z-m>
		<pitch-deg>-28.65</pitch-deg>
		<w>512</w>
		<h>512</h>
	</offsets>
 </model>

