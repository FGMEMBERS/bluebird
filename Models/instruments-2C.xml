 <animation>
	<type>range</type>
	<object-name>COM1A</object-name>
	<object-name>COM1S</object-name>
	<object-name>COM2A</object-name>
	<object-name>COM2S</object-name>
	<min-m>0</min-m>
	<max-m>25</max-m>
 </animation>

 <animation>
	<type>material</type>
	<object-name>com1a.frame</object-name>
	<property-base>sim/model/bluebird/lighting/interior5-console</property-base>
	<emission>
		<red-prop>emission/red</red-prop>
		<green-prop>emission/green</green-prop>
		<blue-prop>emission/blue</blue-prop>
		<factor>1</factor>
	</emission>
	<specular>
		<factor-prop>../interior-specular</factor-prop>
		<red>1</red>
		<green>1</green>
		<blue>1</blue>
	</specular>
 </animation>

