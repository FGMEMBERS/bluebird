 <!-- hotspot picks == buttons and knobs -->
 <animation>
	<type>pick</type>
	<object-name>com1a.knob1L</object-name>
	<action>
		<name>decrease com1a by 0.025</name>
		<button>0</button>
		<repeatable>true</repeatable>
		<binding>
			<command>nasal</command>
			<script>digitalPanel.turn_com1aknob(-0.025)</script>
		</binding>
	</action>
	<hovered>
		<binding>
			<command>set-tooltip</command>
			<tooltip-id>com1a1L</tooltip-id>
			<label>Center knob subtract 0.025</label>
		</binding>
	</hovered>
 </animation>

